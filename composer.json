{
    "name": "fsuuaas/laravel-decomposer",
    "description": "A Laravel package to decompose your installed packages, their dependencies, your app & server environment",
    "license": "MIT",
    "authors": [
        {
            "name": "Harish Toshniwal",
            "email": "harish@lubus.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fsuuaas\\Decomposer\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=6.2"
    },
    "require-dev": {
        "illuminate/routing": ">=6.2",
        "illuminate/http": ">=6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fsuuaas\\Decomposer\\DecomposerServiceProvider"
            ]
        }
    }
}
